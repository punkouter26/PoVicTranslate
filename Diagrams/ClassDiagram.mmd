classDiagram
    class TranslationRequest {
        +string Text
        +int WordCount
        +Validate() bool
    }

    class TranslationResponse {
        +string OriginalText
        +string VictorianText
        +int OriginalWordCount
        +int VictorianWordCount
        +DateTime Timestamp
    }

    class LyricsCollection {
        +string Name
        +List~LyricsItem~ Lyrics
        +GetRandom() LyricsItem
        +GetByTitle(string) LyricsItem
    }

    class LyricsItem {
        +string Title
        +string Artist
        +string Album
        +string Content
        +int WordCount
    }

    class DiagnosticResult {
        +string CheckName
        +bool Success
        +string Message
        +Exception Error
        +DateTime Timestamp
    }

    class ClientLogEntry {
        +string Level
        +string Message
        +string Page
        +string UserId
        +DateTime Timestamp
        +Dictionary AdditionalData
    }

    class ApiSettings {
        +string AzureOpenAIApiKey
        +string AzureOpenAIEndpoint
        +string AzureOpenAIDeploymentName
        +string AzureSpeechSubscriptionKey
        +string AzureSpeechRegion
    }

    class HealthStatus {
        +string Status
        +DateTime Timestamp
        +List~HealthCheck~ Checks
    }

    class HealthCheck {
        +string Name
        +string Status
        +string Description
        +string Error
    }

    LyricsCollection "1" *-- "many" LyricsItem
    HealthStatus "1" *-- "many" HealthCheck
    TranslationRequest ..> TranslationResponse : creates
    DiagnosticResult ..> HealthCheck : similar to
